    add( button1 );
    button1.setShapeName( "button1" );
    button1.setPos( -90, -190 );
    button1.setFillColor( new Color( 13421772 ) );
    button1.setSize( 80, 30 );
    button1.setLineWidth( 1 );
    button1.setLineColor( new Color( 0 ) );
